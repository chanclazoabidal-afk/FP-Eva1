                    <output expression="&quot;El resultado es igual que 0?: &quot;" newline="True"/>
                    <output expression="resultado3" newline="True"/>
                </else>
            </if>
            <if expression="radio&lt;&gt;0">
                <then>
                    <assign variable="resultado4" expression="true"/>
                    <output expression="&quot;El resultado es desigual que 0?: &quot;" newline="True"/>
