    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="area" type="Real" array="False" size=""/>
            <declare name="radio" type="Real" array="False" size=""/>
            <declare name="resultado1" type="Boolean" array="False" size=""/>
            <declare name="resultado2" type="Boolean" array="False" size=""/>
            <declare name="resultado3" type="Boolean" array="False" size=""/>
