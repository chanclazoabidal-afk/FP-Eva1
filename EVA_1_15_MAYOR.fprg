    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="numero1" type="Real" array="False" size=""/>
            <declare name="numero2" type="Real" array="False" size=""/>
            <output expression="&quot;escribe un numero&quot;" newline="True"/>
            <input variable="numero1"/>
            <output expression="&quot;escribe otro numero&quot;" newline="True"/>
            <input variable="numero2"/>
            <if expression="numero1&gt;numero2">
                <then>
                    <output expression="&quot;El primer n&#250;mero es mayor que el segundo&quot;" newline="True"/>
