                </then>
                <else>
                    <if expression="numero1==numero2">
                        <then>
                            <output expression="&quot;El segundo n&#250;mero es igual que el primero&quot;" newline="True"/>
                            <output expression="numero2" newline="True"/>
                            <output expression="&quot;es igual que &quot;" newline="True"/>
                            <output expression="numero1" newline="True"/>
                        </then>
                        <else>
                            <output expression="&quot;El segundo n&#250;mero es mayor que el primero&quot;" newline="True"/>
                            <output expression="numero2" newline="True"/>
                            <output expression="&quot;Es mayor que&quot;" newline="True"/>
